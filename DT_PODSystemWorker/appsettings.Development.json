{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information",
      "DT_PODSystemWorker.Services.PdfExtractionService": "Debug",
      "DT_PODSystemWorker.Services.FormulaCalculationService": "Debug",
      "DT_PODSystemWorker.Services.DocumentProcessorService": "Debug",
      "DT_PODSystemWorker.Services.FileDiscoveryService": "Debug"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Information",
        "System": "Warning",
        "DT_PODSystemWorker.Services": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext:l}: {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/Development/worker-all-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7,
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}",
          "fileSizeLimitBytes": 209715200,
          "rollOnFileSizeLimit": true,
          "restrictedToMinimumLevel": "Debug"
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "SourceContext like 'DT_PODSystemWorker.Services.PdfExtractionService%'"
                }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "Logs/Development/pdf-extraction-.log",
                  "rollingInterval": "Day",
                  "retainedFileCountLimit": 7,
                  "outputTemplate": "[{Timestamp:HH:mm:ss.fff}] {Level:u3} | {Message:lj}{NewLine}{Exception}",
                  "fileSizeLimitBytes": 104857600,
                  "rollOnFileSizeLimit": true
                }
              }
            ]
          }
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "SourceContext like 'DT_PODSystemWorker.Services.FormulaCalculationService%'"
                }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "Logs/Development/formula-calculation-.log",
                  "rollingInterval": "Day",
                  "retainedFileCountLimit": 7,
                  "outputTemplate": "[{Timestamp:HH:mm:ss.fff}] {Level:u3} | {Message:lj}{NewLine}{Exception}",
                  "fileSizeLimitBytes": 104857600,
                  "rollOnFileSizeLimit": true
                }
              }
            ]
          }
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "@m like '%ANCHOR%' or @m like '%CALIBRATION%' or @m like '%COORD%'"
                }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "Logs/Development/anchor-calibration-.log",
                  "rollingInterval": "Day",
                  "retainedFileCountLimit": 7,
                  "outputTemplate": "[{Timestamp:HH:mm:ss.fff}] ANCHOR | {Message:lj}{NewLine}{Exception}",
                  "fileSizeLimitBytes": 52428800,
                  "rollOnFileSizeLimit": true
                }
              }
            ]
          }
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/Development/errors-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30,
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Error",
          "fileSizeLimitBytes": 104857600,
          "rollOnFileSizeLimit": true
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithProcessId"
    ],
    "Properties": {
      "Application": "DT_PODSystemWorker",
      "Environment": "Development"
    }
  }
}